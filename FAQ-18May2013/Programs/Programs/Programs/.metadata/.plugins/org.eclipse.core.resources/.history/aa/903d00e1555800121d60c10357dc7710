package paper2;

public class PassS {
	void start(){
		String s1="slip";
		String s2=fix(s1);
		//System.out.println(s1+""+s2);
	}
	String fix(String s1){
		s1=s1+"strem";
		//System.out.println(s1+"");
		return s1;
	}
	void test(){
		int i=9;
		test1(i);
		System.out.println(i);
	}
	void test1(int i){
		i=i+1;
		
	}

	public static void main(String[] args) {

		PassS p=new PassS();
		p.start();

	}

}
