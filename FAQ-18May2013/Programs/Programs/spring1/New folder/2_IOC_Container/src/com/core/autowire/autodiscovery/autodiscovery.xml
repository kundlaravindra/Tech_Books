<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
>

<context:component-scan base-package="com.core.autowire.autodiscovery">
	

<context:include-filter type="assignable" expression="com.core.autowire.autodiscovery.Instrument"/>

<context:exclude-filter type="annotation" expression="com.core.autowire.autodiscovery.SkipIt"/>
	
	
</context:component-scan>







</beans>
