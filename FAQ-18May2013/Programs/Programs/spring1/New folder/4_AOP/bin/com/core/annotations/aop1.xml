<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:aop="http://www.springframework.org/schema/aop"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.0.xsd">


<aop:aspectj-autoproxy/>

<bean id="audience" class="com.core.annotations.Audience"></bean>

<bean id="performer" class="com.core.annotations.Performer"></bean>

<bean id="logger" class="com.core.annotations.Logger"></bean>

<bean id="songLogger" class="com.core.annotations.SongLogger"></bean>

<bean id="singer" class="com.core.annotations.WesternSinger"></bean>

<bean id="introducer" class="com.core.annotations.InstrumentalistIntroducer"></bean>




</beans>
